{
  "SolarDB": {
    "name": "SolarDB",
    "connector": "memory"
  },

  "GoogleMaps": {
    "name": "GoogleMaps",
    "baseURL": "https://maps.googleapis.com/maps/api/",
    "crud": false,
    "connector": "rest",
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json"
      },
      "strictSSL": true
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "http://maps.googleapis.com/maps/api/geocode/{format=json}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "query": {
            "address": "{street},{city},{zipcode}",
            "sensor": "{sensor=false}"
          },
          "responsePath": "$.results[0].geometry.location"
        },
        "functions": {
          "geocode": [
            "street",
            "city",
            "zipcode"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "https://maps.googleapis.com/maps/api/elevation/{format=json}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "query": {
            "locations": "{locations}"
          },
          "responsePath": "$.results"
        },
        "functions": {
          "getElevation": [
            "locations"
          ]
        }
      }
    ]
  },
  "BlobStore": {
    "name": "BlobStore",
    "baseURL": "https://bucket-16909670-8588-4793-a261-2ffc3a64d3c4.s3-us-west-2.amazonaws.com",
    "crud": false,
    "connector": "rest",
    "options": {
      "strictSSL": true
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://bucket-16909670-8588-4793-a261-2ffc3a64d3c4.s3-us-west-2.amazonaws.com/",
          "headers": {
            "Date": "mockDate",
            "Authorization": "mockAuth"
          }
        },
        "functions": {
          "list": []
        }
      }
    ]
  },
  "NASA": {
    "name": "NASA",
    "baseURL": "https://eosweb.larc.nasa.gov/cgi-bin/sse/grid.cgi",
    "crud": false,
    "connector": "rest",
    "headers": {
      "accepts": "text/html",
      "content-type": "text/html"
    },
    "operations":[
      {
        "template": {
          "method": "GET",
          "url": "https://eosweb.larc.nasa.gov/cgi-bin/sse/grid.cgi",
          "query": {
            "num":"081110",
            "hgt":"100",
            "veg":"17",
            "p":"grid_id",
            "step":"2",
            "lat": "{latitude}",
            "lon": "{longitude}"
          }
        },
        "functions": {
          "getNASAData": ["latitude", "longitude"]
        }
      }
    ]
  }
}
