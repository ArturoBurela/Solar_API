{
  "SolarDB":{
    "name":"SolarDB",
    "connector": "memory"
  },
  "GoogleMaps": {
    "name": "GoogleMaps",
    "baseURL": "https://maps.googleapis.com/maps/api/",
    "crud": false,
    "connector": "rest",
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json"
      },
      "strictSSL": true
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "http://maps.googleapis.com/maps/api/geocode/{format=json}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "query": {
            "address": "{street},{city},{zipcode}",
            "sensor": "{sensor=false}"
          },
          "responsePath": "$.results[0].geometry.location"
        },
        "functions": {
          "geocode": [
            "street",
            "city",
            "zipcode"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "https://maps.googleapis.com/maps/api/elevation/{format=json}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json"
          },
          "query": {
            "locations": "{locations}"
          },
          "responsePath": "$.results"
        },
        "functions": {
          "getElevation": [
            "locations"
          ]
        }
      }
    ]
  },
  "NASA": {
    "name": "NASA",
    "baseURL": "https://eosweb.larc.nasa.gov/cgi-bin/sse/grid.cgi",
    "crud": false,
    "connector": "rest",
    "headers": {
      "accepts": "text/html",
      "content-type": "text/html"
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://eosweb.larc.nasa.gov/cgi-bin/sse/grid.cgi",
          "query": {
            "num": "081110",
            "hgt": "100",
            "veg": "17",
            "p": "grid_id",
            "step": "2",
            "lat": "{latitude}",
            "lon": "{longitude}"
          }
        },
        "functions": {
          "getNASAData": [
            "latitude",
            "longitude"
          ]
        }
      }
    ]
  },
  "SolarAnalytics": {
    "name": "SolarAnalytics",
    "baseURL": "https://predix-analytics-catalog-release.run.aws-usw02-pr.ice.predix.io/",
    "crud": false,
    "connector": "rest",
    "options": {
      "headers": {
        "Predix-Zone-Id": "00c30eb0-8b5e-411c-bc3f-9d3a5d70f0d0",
        "content-type": "application/json"
      },
      "strictSSL": true
    },
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "https://predix-analytics-catalog-release.run.aws-usw02-pr.ice.predix.io/api/v1/catalog/analytics/aa7bbd17-23c0-4bf1-8a53-3ddd13267f68/execution",
          "headers": {
            "Data": "mockData",
            "Authorization": "mockAuth"
          },
          "data": "{data}"
        },
        "functions": {
          "executePhotovoltaic": [
            "data"
          ]
        }
      },
      {
        "template": {
          "method": "POST",
          "url": "https://predix-analytics-catalog-release.run.aws-usw02-pr.ice.predix.io/api/v1/catalog/analytics/86a45079-21ce-4c65-a5e1-2329a18016fb/execution",
          "headers": {
            "Data": "mockData",
            "Authorization": "mockAuth"
          },
          "data": "{data}"
        },
        "functions": {
          "executeCustom": [
            "data"
          ]
        }
      }
    ]
  }
}
